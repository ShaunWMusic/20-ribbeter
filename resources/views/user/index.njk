{% extends "layout" %}

{% block content %}

  <h3 class="header-users">Users</h2>
    <div class="grid">
      {% for user in users %}
        <div class="grid__item">
          <img src="http://www.placecera.com/100/100" alt="">
          <div class="grid__item--content">
            <p>{{ user.username }}</p>
            <p>{{ user.email }}</p>
          </div>
        </div>
      {% endfor %}
    </div>


{% endblock %}
